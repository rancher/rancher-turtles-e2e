{
  "compilerOptions": {
    "target": "ES2018",
    "module": "nodenext",
    "moduleResolution": "nodenext",
    "lib": [
      "esnext",
      "esnext.asynciterable",
      "dom"
    ],
    "esModuleInterop": true,
    "allowJs": true,
    "sourceMap": true,
    "strict": true,
    "noEmit": true,
    "baseUrl": ".",
    "paths": {
      "~/*": [
        "./*"
      ],
      "@/*": [
        "./*"
      ]
    },
    "types": [
      "@types/node",
      "cypress",
      "cy-verify-downloads",
      "@rancher-ecp-qa/cypress-library",
      "cypress-file-upload"
    ]
  },
  "exclude": [
    "node_modules"
  ]
}
